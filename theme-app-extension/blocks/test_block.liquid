<div>
  {%- if block.settings.text -%}
    <h4>{{ block.settings.text | escape }}</h4>
  {%- endif -%}
  {%- render "test_snippet", title: 'Hello world!' -%}
  <div class="stars__div">
    {%- render "stars_snippet" -%}
  </div>
</div>

{% schema %}
{
  "name": "Test block",
  "target": "section",
  "stylesheet": "stars.css",
  "settings": [
    {
      "type": "text",
      "id": "text",
      "label": "Announcement text",
      "default": "Announce something here"
    }
  ]
}
{% endschema %}